{
  "name": "carpo-redspot",
  "version": "0.0.0",
  "description": "carpo extension and vscode extension",
  "main": "build/extension.js",
  "repository": "https://github.com/patractlabs/carpo-extension.git",
  "author": "patractlabs",
  "license": "MIT",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "carpoView",
          "title": "Carpo",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "carpoView": [
        {
          "type": "webview",
          "id": "carpo.compileView",
          "name": "Compile"
        },
        {
          "type": "webview",
          "id": "carpo.runView",
          "name": "Run"
        },
        {
          "type": "webview",
          "id": "carpo.testView",
          "name": "Test"
        }
      ]
    }
  },
  "devDependencies": {
    "@patract/dev": "^0.5.21",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/semver": "^7.3.8",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "markdown-loader": "^6.0.0",
    "mini-css-extract-plugin": "^2.2.2",
    "null-loader": "^4.0.1",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.2.1",
    "thread-loader": "^3.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.0.0",
    "globby": "^12.0.2",
    "semver": "^7.3.5"
  }
}
