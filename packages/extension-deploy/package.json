{
  "name": "carpo-deploy",
  "version": "0.0.4-2",
  "description": "carpo-remix extension and vscode extension",
  "main": "build/extension.js",
  "repository": "https://github.com/patractlabs/carpo-extension.git",
  "author": "patractlabs",
  "license": "MIT",
  "engines": {
    "vscode": "^1.59.0"
  },
  "publisher": "carpo",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionDependencies": [
    "carpo.carpo-core"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "carpoDeploy",
          "title": "Carpo",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "carpoDeploy": [
        {
          "type": "webview",
          "id": "carpoDeploy.deployView",
          "name": "Deploy & Run Transactions"
        }
      ]
    }
  },
  "devDependencies": {
    "vsce": "^2.5.1"
  }
}
