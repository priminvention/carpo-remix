{
  "name": "carpo-compiler",
  "version": "0.0.4-2",
  "description": "carpo-remix extension and vscode extension",
  "main": "build/extension.js",
  "repository": "https://github.com/patractlabs/carpo-extension.git",
  "author": "patractlabs",
  "license": "MIT",
  "engines": {
    "vscode": "^1.59.0"
  },
  "publisher": "carpo",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionDependencies": [
    "carpo.carpo-core"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "carpoCompiler",
          "title": "Carpo Compiler",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "carpo-compiler.compileOne",
        "title": "Compile this file",
        "icon": {
          "light": "./resources/compile-light.svg",
          "dark": "./resources/compile-dark.svg"
        }
      }
    ],
    "views": {
      "carpoCompiler": [
        {
          "type": "webview",
          "id": "carpoCompiler.compileView",
          "name": "Compiler"
        }
      ]
    },
    "menus": {
      "editor/title": [
        {
          "when": "editorFocus && carpo-compiler.sourceViewOpen",
          "command": "carpo-compiler.compileOne",
          "group": "navigation"
        }
      ]
    }
  }
}
