{
  "name": "carpo-compiler",
  "version": "0.0.1-0",
  "description": "carpo-remix extension and vscode extension",
  "main": "build/extension.js",
  "repository": "https://github.com/patractlabs/carpo-extension.git",
  "author": "patractlabs",
  "license": "MIT",
  "engines": {
    "vscode": "^1.59.0"
  },
  "publisher": "carpo",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "extensionDependencies": [
    "carpo.carpo-core"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "carpoCompiler",
          "title": "Carpo Compiler",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "carpoCompiler": [
        {
          "type": "webview",
          "id": "carpoCompiler.compileView",
          "name": "Compiler"
        }
      ]
    }
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.0.0",
    "semver": "^7.3.5"
  }
}
